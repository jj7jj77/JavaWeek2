package javaclassweek2;
import java.util.Scanner;
public class PastPresentFuture 
{
    public static void main(String[] args)
    {
        Scanner jinput = new Scanner(System.in);   
        System.out.print("Enter a month");
        int month = jinput.nextInt();
        System.out.print("Enter a day");
        int day = jinput.nextInt();
        System.out.print("Enter a year");
        int year = jinput.nextInt();
        
        if (year!= 2017)
            System.out.print("Not this year");
        else if (month <9)
            System.out.print("In an earlier month this year");
        else if (month >9)
            System.out.print("In a later month this year");
        else
            System.out.print("This month");
     }
}

//!= not equal
